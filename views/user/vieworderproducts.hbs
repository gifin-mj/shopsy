<table id="orders">
  <tr> 
      <h1 style="text-align: center;">Products List</h1>
  </tr>
  <tr>
    <th>Name</th>
    <th>Category</th>
    <th>Description</th>
    <th>Image</th>
    <th>Price</th>
    <th>Quantity</th>
    <th>Total</th>
  </tr>
{{#each orderproducts}}
  <tr>
    <td> {{this.product.pname}}</td>
    <td> {{this.product.cat}} </td>
    <td> {{this.product.description}} </td>
    <td>  <img src="/product-images/{{this.product._id}}.jpg" width="50px" height="50px" alt="qwqwe" />  </td>
    <td> {{this.product.price}} </td>
    <td> {{this.quantity}} </td>
    <td> {{#multiply this.product.price this.quantity }}{{/multiply}} </td>  
  </tr>
{{/each}}
  </table>
  